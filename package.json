{
  "name": "keyflowos-monorepo",
  "name": "KEYFLOWOS-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --parallel --stream -r dev",
    "lint": "pnpm -r lint",
    "db:generate": "pnpm --filter db run db:generate",
    "db:push": "pnpm --filter db exec prisma db push",
    "db:studio": "pnpm --filter db exec prisma studio",
    "ui:dev": "pnpm --filter ui exec storybook dev -p 6006",
    "test:db:soft-delete": "pnpm --filter db exec tsx --env-file .env src/test-soft-delete.ts"
  },
  "devDependencies": {
    "typescript": "^5.4.5",
    "prettier": "^3.2.5",
    "eslint": "^9.0.0",
    "turbo": "latest",
    "tsx": "^4.20.6",
    "autoprefixer": "^10.4.20",
    "prisma": "^5.13.0"
  },
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=20 <21"
  },
  "pnpm": {
    "overrides": {
      "baseline-browser-mapping": "^2.8.32"
    }
  }
}
