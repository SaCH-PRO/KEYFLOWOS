{
  "name": "keyflow-os-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm --filter \"./**\" build",
    "dev": "pnpm --parallel --stream --filter \"./**\" dev",
    "lint": "pnpm --filter \"./**\" lint",
    "db:generate": "pnpm --filter @keyflow/db exec prisma generate",
    "db:push": "pnpm --filter @keyflow/db exec prisma db push",
    "db:studio": "pnpm --filter @keyflow/db exec prisma studio",
    "ui:dev": "pnpm --filter @keyflow/ui exec storybook dev -p 6006"
  },
  "devDependencies": {
    "typescript": "^5.4.5",
    "prettier": "^3.2.5",
    "eslint": "^8.57.0",
    "turbo": "latest"
  }
}