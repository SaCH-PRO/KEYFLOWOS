{
  "name": "keyflowos-monorepo",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --parallel --stream -r dev",
    "lint": "pnpm -r lint",
    "db:generate": "pnpm --filter db run db:generate",
    "db:push": "pnpm --filter db exec prisma db push",
    "db:studio": "pnpm --filter db prisma studio",
    "ui:dev": "pnpm --filter ui storybook",
    "test:db:soft-delete": "pnpm --filter db exec tsx --env-file .env src/test-soft-delete.ts"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.20.6",
    "turbo": "latest",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=20 <21"
  }
}